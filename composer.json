{
	"name" : "stinger-soft/doctrine-commons",
	"type" : "library",
	"description" : "Helper functions and DQL Extensions for Doctrine",
	"keywords" : [
		"Doctrine",
		"Commons"
	],
	"homepage" : "http://www.stinger-soft.net",
	"authors" : [{
			"name" : "Florian Meyer",
			"email" : "florian.meyer@stinger-soft.net",
			"role" : "Developer"
		}
	],
	"autoload" : {
		"psr-4" : {
			"StingerSoft\\DoctrineCommons\\" : "src/StingerSoft/DoctrineCommons"
		},
		"exclude-from-classmap" : [
			"/Tests/"
		]
	},
	"license" : "Apache-2.0",
	"require" : {
		"php" : ">=5.5",
		"doctrine/orm" : ">=2.5.0",
		"doctrine/common" : ">=2.5.0",
		"symfony/translation" : "^2.8|^3.0",
		"symfony/http-kernel" : "^2.8|^3.0"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~4.1|~5.0",
		"satooshi/php-coveralls" : "~0.7.1"
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "0.1.x-dev",
			"dev-develop" : "1.0.x-dev"
		}
	}
}