{
  "name": "stinger-soft/doctrine-commons",
  "type": "library",
  "description": "Helper functions and DQL Extensions for Doctrine",
  "keywords": [
    "Doctrine",
    "Commons"
  ],
  "homepage": "http://www.stinger-soft.net",
  "authors": [
    {
      "name": "Florian Meyer",
      "email": "florian.meyer@stinger-soft.net",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "StingerSoft\\DoctrineCommons\\": "src/StingerSoft/DoctrineCommons"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "StingerSoft\\DoctrineCommons\\": "tests/StingerSoft/DoctrineCommons",
      "StingerSoft\\TestBundle\\": "tests/StingerSoft/TestBundle"
    }
  },
  "license": "Apache-2.0",
  "require": {
    "php": "^7.4||^8.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "doctrine/common": "^3.0",
    "doctrine/dbal": "^2.11",
    "doctrine/event-manager": "^1.1",
    "doctrine/persistence": "^2.3",
    "doctrine/orm": "^2.11",
    "salsify/json-streaming-parser": "^8.0",
    "symfony/console": "^5.0||^6.0",
    "symfony/http-kernel": "^5.0||^6.0",
    "symfony/translation-contracts" : "^2.0"
  },
  "require-dev": {
    "doctrine/annotations": "^1.13",
    "ocramius/proxy-manager": "^2.13",
    "phpunit/phpunit" : "^9.5",
    "php-coveralls/php-coveralls" : "^2.5",
    "symfony/phpunit-bridge" : "^5.0||^6.0",
    "stof/doctrine-extensions-bundle": "^1.3"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev",
      "dev-develop": "1.1.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "ocramius/package-versions": false
    }
  }
}
